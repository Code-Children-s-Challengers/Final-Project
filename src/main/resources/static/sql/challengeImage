create table challengeImage
(
    cnum            number(10),
    mimetype      varchar(100)                          null,
    data          blob                        null,
    original_name varchar(100)                          null
);

create table cPhotoImage
(
  cnum number(10),
  unum number(10), 
  mimetype varchar2(100) null,
  data blob null,
  original_name varchar2(100) null,
  c_comment VARCHAR2(25 BYTE) DEFAULT NULL, 
  uploaddate varchar2(200), 
  validates NUMBER(10) DEFAULT 1
);