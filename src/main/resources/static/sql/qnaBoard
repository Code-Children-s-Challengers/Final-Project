create table qnaBoard
(
  id number primary key,
  title nvarchar2(100),
  writerid NVARCHAR2(50),
  qcontent NVARCHAR2(1000),
  acontent NVARCHAR2(1000),
  regdate QnABoardSearch,
  hit number,
  files nvarchar2(1000),
  answeryn nvarchar2(50)
  type nvarchar2(100),
  keyword nvarchar2(100)
  
);
create table qnaBoard
(
  id number primary key,
  title nvarchar2(100),
  writerid NVARCHAR2(50),
  qcontent NVARCHAR2(1000),
  acontent NVARCHAR2(1000),
  regdate date default null,  
  hit number default 0,
  files nvarchar2(1000) default null,
  type nvarchar2(100) default null,
  keyword nvarchar2(100) default null,
  date1 nvarchar2(50),
  tate2 nvarchar2(50),
  modidate date default null,
  reguname date default null,
  modiuname date default null  
);

create sequence qnaboard_id_seq
    START WITH 1
    INCREMENT BY 1;
 