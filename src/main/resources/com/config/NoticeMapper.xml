<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.config.NoticeMapper">
	<select id="selectAllNotice" resultType = "NoticeDTO">
		select *
		from notice
	</select>
	
	<select id="selectNoticeContent" resultType = "NoticeDTO" parameterType="int">
		select *
		from notice
		where id = #{id}
	</select>
	
	<insert id="insertNotice" parameterType="NoticeDTO">
		insert into notice(id,title,writeid,content,regdate,hit,files)
		values (#{id},
				#{title},
				#{writeid},
				#{content},
				#{regdate},
				#{hit},
				#{files}			
		)
		
	</insert>
	
</mapper>